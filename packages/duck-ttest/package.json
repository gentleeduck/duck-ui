{
  "$schema": "https://json.schemastore.org/package.json",
  "name": "@gentleduck/ttest",
  "version": "0.4.1",
  "description": "Compile-time type testing utilities for TypeScript",
  "type": "module",
  "exports": {
    "./*": "./src/*/index.ts"
  },
  "files": [
    "src",
    "README.md",
    "LICENSE",
    "CHANGELOG.md",
    "SECURITY.md",
    "article-about-this.md",
    "tsconfig.json"
  ],
  "scripts": {
    "test": "tsc --noEmit",
    "lint": "biome lint --write ./",
    "format": "biome format --write ./",
    "ci": "pnpm run lint && pnpm run format && pnpm run test",
    "release": "changeset version",
    "publish": "pnpm run ci && changeset publish"
  },
  "keywords": [
    "type",
    "testing",
    "typescript",
    "compile-time",
    "types"
  ],
  "author": "wildduck",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/gentleeduck/ui",
    "directory": "packages/duck-ttest"
  },
  "bugs": {
    "url": "https://github.com/gentleeduck/ui/issues"
  },
  "homepage": "https://github.com/gentleeduck/ui/tree/master/packages/duck-ttest#readme",
  "peerDependencies": {
    "@types/node": "catalog:"
  },
  "engines": {
    "node": ">=24.2.0"
  },
  "packageManager": "pnpm@10.11.0"
}
