{
  "$schema": "https://json.schemastore.org/package.json",
  "name": "@gentleduck/ttest",
  "version": "0.2.0",
  "description": "A package for type testing",
  "type": "module",
  "types": "./dist/**/*.d.ts",
  "main": "./dist/index.js",
  "exports": {
    ".": "./dist/index.js",
    "./*": "./dist/*/index.js",
    "./examples/*": "./src/examples/*"
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "type-test": "tsc --noEmit",
    "build": "tsdown",
    "lint": "biome lint --write ./",
    "format": "biome format --write ./",
    "ci": "pnpm run lint && pnpm run format && pnpm run build",
    "release": "changeset version",
    "publish": "pnpm run ci && changeset publish"
  },
  "keywords": [
    "react",
    "components",
    "performance"
  ],
  "author": "wilddcuk",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/gentleduck/ui/tree/master/packages/duck-ttest"
  },
  "bugs": {
    "url": "https://github.com/gentleduck/ui/issues"
  },
  "homepage": "https://github.com/gentleduck/ui/tree/master/packages/duck-ttest#readme",
  "dependencies": {
    "@types/node": "catalog:"
  },
  "devDependencies": {
    "@changesets/cli": "catalog:",
    "@gentleduck/tsdown-config": "workspace:*",
    "@types/react": "catalog:react19",
    "@types/react-dom": "catalog:react19",
    "husky": "catalog:",
    "tsdown": "catalog:"
  },
  "packageManager": "pnpm@10.10.0",
  "engines": {
    "node": ">=22.0.0"
  }
}
